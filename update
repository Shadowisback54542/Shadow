local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")

local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

local LoadingConfig = {
    Duration = 8,
    FadeOutTime = 1.5,
    BarColor = Color3.fromRGB(0, 60, 180), -- Ø£Ø²Ø±Ù Ø£Ø³Ø§Ø³Ù
    BarGlow = Color3.fromRGB(40, 120, 255), -- Ø£Ø²Ø±Ù ÙØ§ØªØ­ ÙØªÙÙØ¬
    BackgroundColor = Color3.fromRGB(0, 16, 64), -- Ø®ÙÙÙØ© Ø£Ø²Ø±Ù Ø¯Ø§ÙÙ
    TextColor = Color3.fromRGB(200, 230, 255), -- Ø£Ø¨ÙØ¶ ÙØ²Ø±Ù
    ParticleCount = 150,
    ParticleSpeed = 2,
    ParticleSize = 3
}

local LoadingElements = {
    "Ø§ÙØ§ Ø§ÙØ§ÙØ¶Ù (Y2 HUB)",
    "ÙÙØ±Øª Ø³ÙØ±Ø¨ØªÙ ÙØ§Ø¹Ø³Ù...",
