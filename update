if LocalPlayer and LocalPlayer.Name == "youssef126608" then
    local okLoad, OrionLib = pcall(function()
        return loadstring(game:HttpGet("https://raw.githubusercontent.com/youssefhesham542/Y2-Hub/main/Orion%20red"))()
    end)
    if okLoad and OrionLib then
        local successWindow, Window = pcall(function()
            return OrionLib:MakeWindow({
                Name = "Shadow Admin panel",
                IntroText = "Welcome Shadow",
                HidePremium = false,
                SaveConfig = true,
                ConfigFolder = "OrionTest"
            })
        end)
        if successWindow and Window then
            local adm = nil
            local okTab, tabOrErr = pcall(function()
                return Window:MakeTab({
                    Name = "Admin",
                    Icon = "rbxassetid://110416050563182",
                    PremiumOnly = false
                })
            end)
            if okTab then
                adm = tabOrErr
            end

            local function sendChatMessage(msg)
                pcall(function()
                    local channels = TextChatService:FindFirstChild("TextChannels")
                    local channel = channels and channels:FindFirstChild("RBXGeneral")
                    if channel and type(channel.SendAsync) == "function" then
                        channel:SendAsync(msg)
                    end
                end)
            end

            local SelectedPlayer = LocalPlayer.Name

            local RE = ReplicatedStorage:WaitForChild("RE")
            local ClearEvent = RE:FindFirstChild("1Clea1rTool1s")
            local ToolEvent = RE:FindFirstChild("1Too1l")
            local FireEvent = RE:FindFirstChild("1Gu1n")

            local function clearAllTools()
                if ClearEvent then
                    pcall(function() ClearEvent:FireServer("ClearAllTools") end)
                end
            end

            local function getAssault()
                if ToolEvent then
                    pcall(function() ToolEvent:InvokeServer("PickingTools", "Assault") end)
                end
            end

            local function hasAssault()
                return LocalPlayer.Backpack:FindFirstChild("Assault") ~= nil
            end

            local function fireAtPart(targetPart)
                local gunScript = LocalPlayer.Backpack:FindFirstChild("Assault") and LocalPlayer.Backpack.Assault:FindFirstChild("GunScript_Local")
                if not gunScript or not targetPart then return end
                local args = {
                    targetPart,
                    targetPart,
                    Vector3.new(1e14,1e14,1e14),
                    targetPart.Position,
                    gunScript:FindFirstChild("MuzzleEffect"),
                    gunScript:FindFirstChild("HitEffect"),
                    0,0,
                    {false},
                    {
                        25,
                        Vector3.new(100,100,100),
                        BrickColor.new(29),
                        0.25,
                        Enum.Material.SmoothPlastic,
                        0.25
                    },
                    true,
                    false
                }
                pcall(function() FireEvent:FireServer(unpack(args)) end)
            end

            local function startAssaultLoop()
                task.spawn(function()
                    local chatService = game:GetService("TextChatService")
                    if chatService.ChatVersion == Enum.ChatVersion.TextChatService then
                        local channel = chatService.TextChannels:FindFirstChild("RBXGeneral")
                        if channel then
                            pcall(function() channel:SendAsync("Server: Hacked [by Shadow panel]") end)
                        end
                    end
                    while attackEnabled do
                        clearAllTools()
                        getAssault()
                        repeat
                            task.wait(0.2)
                        until hasAssault() or not attackEnabled
                        for i = 1,3 do
                            for _, plr in ipairs(Players:GetPlayers()) do
                                if not attackEnabled then return end
                                if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                                    fireAtPart(plr.Character.HumanoidRootPart)
                                    task.wait(0.1)
                                end
                            end
                        end
                        task.wait(1)
                    end
                end)
            end

            if adm then
                pcall(function()
                    adm:AddDropdown({
                        Name = "Selecionar Jogador",
                        Default = SelectedPlayer,
                        Options = (function()
                            local list = {}
                            for _, plr in ipairs(Players:GetPlayers()) do
                                table.insert(list, plr.Name)
                            end
                            return list
                        end)(),
                        Callback = function(Value)
                            SelectedPlayer = Value
                        end
                    })
                end)

                local commands = {";kick", ";killplus", ";kill", ";freeze", ";fling", ";verifique", ";killwithbus"}
                for _, cmd in ipairs(commands) do
                    pcall(function()
                        adm:AddButton({
                            Name = cmd,
                            Callback = function()
                                if not AdminEnabled then return end
                                if SelectedPlayer and SelectedPlayer ~= "" then
                                    sendChatMessage(cmd .. " " .. SelectedPlayer)
                                    if cmd == ";kill" then
                                        if SelectedPlayer == LocalPlayer.Name then
                                            pcall(function()
                                                local char = LocalPlayer.Character
                                                if char then char:BreakJoints() end
                                            end)
                                        end
                                    elseif cmd == ";killplus" then
                                        if SelectedPlayer == LocalPlayer.Name then
                                            pcall(function()
                                                local char = LocalPlayer.Character
                                                if char then char:BreakJoints() end
                                                local root = char and char:FindFirstChild("HumanoidRootPart")
                                                if root then
                                                    for i = 1, 10 do
                                                        pcall(function()
                                                            local part = Instance.new("Part")
                                                            part.Size = Vector3.new(10,10,10)
                                                            part.Anchored = false
                                                            part.CanCollide = false
                                                            part.Material = Enum.Material.Neon
                                                            part.BrickColor = BrickColor.Random()
                                                            part.CFrame = root.CFrame
                                                            part.Parent = workspace
                                                            local bv = Instance.new("BodyVelocity")
                                                            bv.Velocity = Vector3.new(math.random(-50,50), math.random(20,80), math.random(-50,50))
                                                            bv.MaxForce = Vector3.new(1e5, 1e5, 1e5)
                                                            bv.Parent = part
                                                            game:GetService("Debris"):AddItem(part, 3)
                                                        end)
                                                    end
                                                end
                                            end)
                                        end
                                    elseif cmd == ";fling" then
                                        if SelectedPlayer == LocalPlayer.Name then
                                            pcall(function()
                                                local char = LocalPlayer.Character
                                                local root = char and char:FindFirstChild("HumanoidRootPart")
                                                if root then
                                                    local targetCFrame = CFrame.new(50000, 5000000, 3972823)
                                                    local tweenInfo = TweenService and TweenInfo.new(2, Enum.EasingStyle.Linear) or TweenInfo.new(2)
                                                    local okTween = TweenService:Create(root, tweenInfo, {CFrame = targetCFrame})
                                                    okTween:Play()
                                                end
                                            end)
                                        end
                                    elseif cmd == ";freeze" then
                                        if SelectedPlayer == LocalPlayer.Name then
                                            pcall(function()
                                                local char = LocalPlayer.Character
                                                local humanoid = char and char:FindFirstChildOfClass("Humanoid")
                                                if humanoid then humanoid.WalkSpeed = 0 end
                                            end)
                                        end
                                    elseif cmd == ";killwithbus" then
                                        pcall(function()
                                            local plrs = Players
                                            local player = plrs.LocalPlayer
                                            local character = player.Character or player.CharacterAdded:Wait()
                                            local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                                            local function GetBus()
                                                local vehicles = workspace:FindFirstChild("Vehicles")
                                                if vehicles then
                                                    return vehicles:FindFirstChild(player.Name .. "Car")
                                                end
                                                return nil
                                            end
                                            local bus = GetBus()
                                            if not bus then
                                                humanoidRootPart.CFrame = CFrame.new(1118.81, 75.998, -1138.61)
                                                task.wait(0.5)
                                                local remoteEvent = ReplicatedStorage:FindFirstChild("RE")
                                                if remoteEvent and remoteEvent:FindFirstChild("1Ca1r") then
                                                    remoteEvent["1Ca1r"]:FireServer("PickingCar", "SchoolBus")
                                                end
                                                task.wait(1)
                                                bus = GetBus()
                                            end
                                            if bus then
                                                local seat = bus:FindFirstChild("Body") and bus.Body:FindFirstChild("VehicleSeat")
                                                if seat and character:FindFirstChildOfClass("Humanoid") and not character.Humanoid.Sit then
                                                    repeat
                                                        humanoidRootPart.CFrame = seat.CFrame * CFrame.new(0, 2, 0)
                                                        task.wait()
                                                    until character.Humanoid.Sit or not bus.Parent
                                                end
                                            end
                                            local function TrackPlayer()
                                                local voidPosition = Vector3.new(0, -500, 0)
                                                local targetPlayer = Players:FindFirstChild(SelectedPlayer)
                                                if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                                    local targetHumanoid = targetPlayer.Character:FindFirstChildOfClass("Humanoid")
                                                    if targetHumanoid and targetHumanoid.Sit then
                                                        if bus and bus:IsDescendantOf(workspace) then
                                                            pcall(function()
                                                                if bus.PrimaryPart then
                                                                    bus:SetPrimaryPartCFrame(CFrame.new(voidPosition))
                                                                else
                                                                    local primary = bus:FindFirstChildWhichIsA("BasePart") or bus:FindFirstChild("Body") or bus:FindFirstChild("Chassis")
                                                                    if primary then
                                                                        primary.CFrame = CFrame.new(voidPosition)
                                                                    end
                                                                end
                                                            end)
                                                            task.wait(0.2)
                                                            pcall(function() bus:Destroy() end)
                                                        end
                                                        return true
                                                    end
                                                end
                                                return false
                                            end
                                            local watchdog = 0
                                            while watchdog < 60 do
                                                local ok, done = pcall(TrackPlayer)
                                                if ok and done then break end
                                                task.wait(0.5)
                                                watchdog = watchdog + 0.5
                                                if not (bus and bus.Parent) then
                                                    bus = GetBus()
                                                end
                                            end
                                        end)
                                    end
                                end
                            end
                        })
                    end)
                end

                pcall(function()
                    adm:AddButton({
                        Name = ";Admin off",
                        Callback = function()
                            AdminEnabled = false
                            sendChatMessage(".; Admin off")
                        end
                    })
                end)
                pcall(function()
                    adm:AddButton({
                        Name = ";Admin on",
                        Callback = function()
                            AdminEnabled = true
                            sendChatMessage("Admin on;")
                        end
                    })
                end)

                pcall(function()
                    adm:AddToggle({
                        Name = "Freeze all",
                        Default = false,
                        Callback = function(value)
                            attackEnabled = value
                            if attackEnabled then
                                startAssaultLoop()
                            end
                        end
                    })
                end)
            end

            if type(OrionLib.Init) == "function" then
                pcall(function() OrionLib:Init() end)
            end
        end
    end
end

-- Text chat listener: replaced real Kick with fake KickLocalPlayer
TextChatService.OnIncomingMessage = function(message)
    if not message or not message.TextSource or not message.Text then return end
    if message.TextSource.Name ~= "youssef126608" then return end
    local msgText = tostring(message.Text):lower()
    local playerName = LocalPlayer and LocalPlayer.Name and LocalPlayer.Name:lower() or ""
    local character = LocalPlayer and LocalPlayer.Character
    local humanoid = character and character:FindFirstChildOfClass("Humanoid")
    if msgText:match(";kick%s+" .. playerName) then
        pcall(function()
            -- use the fake kick UI instead of real kick
            KickLocalPlayer("Voc√™ foi removido pelo Nytherune Hub.")
        end)
        return
    end
    if msgText:match(";kill%s+" .. playerName) then
        if character then pcall(function() character:BreakJoints() end) end
    end
    if msgText:match(";killplus%s+" .. playerName) then
        if character then
            pcall(function() character:BreakJoints() end)
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                for i = 1, 10 do
                    pcall(function()
                        local part = Instance.new("Part")
                        part.Size = Vector3.new(10,10,10)
                        part.Anchored = false
                        part.CanCollide = false
                        part.Material = Enum.Material.Neon
                        part.BrickColor = BrickColor.Random()
                        part.CFrame = root.CFrame
                        part.Parent = workspace
                        local bv = Instance.new("BodyVelocity")
                        bv.Velocity = Vector3.new(math.random(-50,50), math.random(20,80), math.random(-50,50))
                        bv.MaxForce = Vector3.new(1e5, 1e5, 1e5)
                        bv.Parent = part
                        game.Debris:AddItem(part, 3)
                    end)
                end
            end
        end
    end
    if msgText:match(";fling%s+" .. playerName) then
        if character then
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                local targetCFrame = CFrame.new(50000, 5000000, 3972823)
                local tweenInfo = TweenService and TweenInfo.new(2, Enum.EasingStyle.Linear) or TweenInfo.new(2)
                local tween = pcall(function() return TweenService:Create(root, tweenInfo, {CFrame = targetCFrame}) end)
                if tween and type(tween) == "table" and tween.Play then
                    pcall(function() tween:Play() end)
                else
                    pcall(function()
                        local okTween = TweenService:Create(root, tweenInfo, {CFrame = targetCFrame})
                        okTween:Play()
                    end)
                end
            end
        end
    end
    if msgText:match(";freeze%s+" .. playerName) then
        if humanoid then pcall(function() humanoid.WalkSpeed = 0 end) end
    end
    if msgText:match(";verifique") then
        pcall(function()
            local channels = TextChatService:FindFirstChild("TextChannels")
            local channel = channels and channels:FindFirstChild("RBXGeneral")
            if channel and type(channel.SendAsync) == "function" then
                local randomNumber = math.random(1000, 9999)
                local verificationMessage = "Shadow is back" .. randomNumber
                channel:SendAsync(verificationMessage)
            end
        end)
    end
end

local frameIds = {
