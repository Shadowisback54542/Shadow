{Name="ŸÖŸÉÿßŸÜ ÿßŸÑÿ£ÿ≥ŸÑÿ≠ÿ©", CFrame=CFrame.new(-119, -27, 235)},
{Name="ÿ®Ÿäÿ™ ÿßŸÑÿ±ÿ¨ŸÑ ÿßŸÑÿπÿ¨Ÿàÿ≤", CFrame=CFrame.new(986, 4, 63)}
}

function playTeleportEffect()
blur = Instance.new("BlurEffect")
blur.Size = 0
blur.Parent = game.Lighting
for i = 1, 10 do blur.Size = i * 2 task.wait(0.01) end
task.wait(0.1)
for i = 10, 1, -1 do blur.Size = i * 2 task.wait(0.01) end
blur:Destroy()
end

for _, loc in pairs(TeleportLocations) do
TeleportTab:AddButton({Name = loc.Name, Description = "ÿßÿ∞Ÿáÿ® ÿ•ŸÑŸâ " .. loc.Name, Callback = function()
player = game.Players.LocalPlayer
if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
player.CharacterAdded:Wait()
repeat task.wait() until player.Character and player.Character:FindFirstChild("HumanoidRootPart")
end
playTeleportEffect()
player.Character.HumanoidRootPart.CFrame = loc.CFrame
end})
end

